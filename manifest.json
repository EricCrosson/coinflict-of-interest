{
	"name": "Coinbias",
	"version": "0.0.1",
	"description": "Show user biases on crypto Twitter.",
	"homepage_url": "https://github.com/lukechilds/coinbias",
	"manifest_version": 2,
	"minimum_chrome_version": "36",
	"content_scripts": [
		{
			"run_at": "document_end",
			"matches": ["https://twitter.com/*"],
			"js": ["src/content.js"]
		}
	]
}
