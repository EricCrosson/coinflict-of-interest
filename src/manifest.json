{
	"name": "Coinflict of Interest",
	"version": "1.0.0",
	"icons": {
		"256": "icon.png"
	},
	"description": "Show user biases on Crypto Twitter.",
	"homepage_url": "https://github.com/lukechilds/coinflict-of-interest",
	"manifest_version": 2,
	"minimum_chrome_version": "36",
	"content_scripts": [
		{
			"run_at": "document_end",
			"matches": ["https://twitter.com/*"],
			"js": ["content.js"]
		}
	]
}
